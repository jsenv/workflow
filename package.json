{
  "name": "@jsenv/workflow",
  "version": "0.0.1",
  "private": true,
  "description": "Automate workflows in projects using JavaScript",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/workflow"
  },
  "workspaces": [
    "./packages/*"
  ],
  "exports": {
    "./*": "./*"
  },
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs",
    "test": "npm run test --workspaces --if-present",
    "workspace:versions": "node ./scripts/workspace_versions.mjs",
    "workspace:publish": "node ./scripts/workspace_publish.mjs",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@jsenv/assert": "2.8.1",
    "@jsenv/eslint-config": "16.3.9",
    "@jsenv/eslint-import-resolver": "0.4.10",
    "@jsenv/log": "3.3.4",
    "@jsenv/package-workspace": "./packages/packages-workspace",
    "@jsenv/test": "1.1.2",
    "eslint": "8.40.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "prettier": "2.8.8"
  }
}
