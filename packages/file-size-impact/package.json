{
  "name": "@jsenv/file-size-impact",
  "version": "14.1.1",
  "description": "Add files size impact into pull requests",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/workflow",
    "directory": "packages/file-size-impact"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "bin": {
    "filesize": "./bin/filesize.mjs"
  },
  "main": "./src/main.js",
  "files": [
    "/src/"
  ],
  "scripts": {
    "snapshot": "node ./tests/comment/generate_comment_snapshot_file.mjs",
    "test": "node ./scripts/test.mjs",
    "file-size": "node ./scripts/file_size.mjs --log"
  },
  "dependencies": {
    "@jsenv/dynamic-import-worker": "1.1.0",
    "@jsenv/filesystem": "4.2.3",
    "@jsenv/github-pull-request-impact": "1.7.2",
    "@jsenv/log": "3.3.4",
    "@jsenv/urls": "2.0.0"
  }
}
