{
  "name": "@jsenv/file-size-impact",
  "version": "12.1.7",
  "description": "Add files size impact into pull requests",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/workflow",
    "directory": "packages/jsenv-file-size-impact"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js"
    },
    "./*": "./*"
  },
  "main": "./main.js",
  "files": [
    "/src/",
    "/main.js"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs",
    "importmap": "node ./script/importmap/importmap.mjs",
    "snapshot": "node ./test/comment/generate_comment_snapshot_file.mjs",
    "test": "node ./script/test/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "file-size": "node ./script/file_size/file_size.mjs --log",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@jsenv/dynamic-import-worker": "1.0.1",
    "@jsenv/filesystem": "2.7.1",
    "@jsenv/github-pull-request-impact": "1.6.9",
    "@jsenv/logger": "4.0.1",
    "pretty-bytes": "5.6.0"
  },
  "devDependencies": {
    "@jsenv/assert": "2.4.1",
    "@jsenv/core": "25.3.0",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/importmap-eslint-resolver": "5.2.5",
    "@jsenv/importmap-node-module": "5.1.3",
    "eslint": "8.7.0",
    "eslint-plugin-import": "2.25.4",
    "prettier": "2.5.1"
  }
}
