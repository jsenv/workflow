      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/src/"
  ],
  "scripts": {
    "test": "node --experimental-json-modules ./scripts/test.mjs"
  },
  "dependencies": {
    "@jsenv/filesystem": "4.9.8",
    "@jsenv/log": "3.5.2",
    "semver": "7.6.3"
  }
}
